<p id="notice"><%= notice %></p>

<h1>Api V1 Drafts</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Datetime</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_v1_drafts.each do |api_v1_draft| %>
      <tr>
        <td><%= api_v1_draft.name %></td>
        <td><%= api_v1_draft.datetime %></td>
        <td><%= link_to 'Show', api_v1_draft %></td>
        <td><%= link_to 'Edit', edit_api_v1_draft_path(api_v1_draft) %></td>
        <td><%= link_to 'Destroy', api_v1_draft, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Api V1 Draft', new_api_v1_draft_path %>
