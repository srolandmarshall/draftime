<p id="notice"><%= notice %></p>

<h1>Api V1 Leagues</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_v1_leagues.each do |api_v1_league| %>
      <tr>
        <td><%= api_v1_league.name %></td>
        <td><%= link_to 'Show', api_v1_league %></td>
        <td><%= link_to 'Edit', edit_api_v1_league_path(api_v1_league) %></td>
        <td><%= link_to 'Destroy', api_v1_league, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Api V1 League', new_api_v1_league_path %>
